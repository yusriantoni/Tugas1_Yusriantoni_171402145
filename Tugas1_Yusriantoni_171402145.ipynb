    "class Fibonacci:\n",
    "    def fibo(a, n):\n",
    "        if n == 0:\n",
    "            return 0\n",
    "        elif n == 1:\n",
    "            return 1\n",
    "        else:\n",
    "            return a.fibo(n-1) + a.fibo(n-2)\n",
    "\n",
    "f = Fibonacci()\n",
    "for i in range (5):\n",
    "    print(f.fibo(i))"
   ]
